<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="post" action="/" method="post">
  <div class="mb-3 mt-3">
    <label for="" class="form-label">Title</label>
    <input type="text" class="form-control" name="newTitle">
  </div>
  <div class="mb-3 mt-3">
    <label for="" class="form-label">Post</label>
    <textarea name="newPost" rows="5" cols="80" class="form-control"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>


</form>


<%- include("partials/footer") -%>
